/*
Drosophila embryo simulation
	units are micron-milisecond
*/

graphics opengl # _good

dim 3

# parameters
define_global	length	500
define_global	radius	100
define_global 	cyliner_end	400	#should = length-radius
define_global	mbicoid_num	100
define_global	pbicoid_dif	100
define_global	bicoid_trans	100

#load files
read_file species-appearance
read_file cell-shape
read_file species-properties

time_start 0
time_stop 5000
time_step 1								

end_file
