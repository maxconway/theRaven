/*
Drosophila embryo simulation
	units are micron-second
*/

dim 3

# species list
species mbcd pbcd nucleus

#load files
read_file parameters
read_file cell-shape
read_file cell-core
read_file species-properties
read_file runtime

time_start 0
time_stop endtime
time_step 5

#output
output_root ../results/
output_files pbcd_profile

#logging
read_file logging

#graphics
read_file appearance

end_file
