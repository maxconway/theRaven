##properties of caudal
#diffusion
difc	mcad	mcad_dif
difc	pcad	pcad_dif

#creation
compartment_mol	mcad_num	mcad	cytoplasm

#reactions
reaction_cmpt	cytoplasm	mcadbind	pbcd + mcad -> boundmcad	mcad_binding
reaction_cmpt	cytoplasm	mcadunbind	boundmcad -> mcad + pbcd	mcad_unbinding
reaction_cmpt	cytoplasm	cad-trans	mcad -> mcad + pcad	cad_trans
reaction_cmpt	cytoplasm	pcad-deg	pcad -> 0	pcad_deg

#logging mcad
define molecule mcad
cmd @ 540	echo name.result "molecule "
cmd @ 540	molcountspace molecule 0 0 length buckets -radius radius -radius radius 0 name.result
cmd @ 1080	echo name.result "molecule "
cmd @ 1080	molcountspace molecule 0 0 length buckets -radius radius -radius radius 0 name.result
